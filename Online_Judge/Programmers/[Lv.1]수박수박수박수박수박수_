#include <string>
#include <vector>

using namespace std;

string solution(int n) {
    vector<string> v;
    string answer;
    
    for(int i=0; i<n; i++){
        if((i%2) == 0){
            v.push_back("수");
        }
        else{
            v.push_back("박");
        }
    }
    
    for(string s : v){
        answer += s;
    }
    
    return answer;
}
