#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	freopen("input.txt", "r", stdin);

	int T, N, tmp, ans;

	cin >> T;
	for (int test = 0; test < T; test++) {
		cin >> N;
		ans = 0;
		for (int i = 0; i < N; i++) {
			cin >> tmp;
			ans += tmp;
		}
		cout << ans << endl;
	}

	return 0;
}
