#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int A, B, C, D;

	cin >> A >> B >> C >> D;

	(A + D) <= (B + C) ? cout << A + D : cout << B + C;

	return 0;
}
