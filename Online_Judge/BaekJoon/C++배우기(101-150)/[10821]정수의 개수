#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int cnt = 1;
	string s;

	cin >> s;

	for (int i = 0; i < s.size(); i++) {
		if (s[i] == ',')
			cnt++;
	}

	cout << cnt;

	return 0;
}
