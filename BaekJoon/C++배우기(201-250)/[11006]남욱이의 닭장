#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int testCase, N, M, U, T;

	cin >> testCase;

	for (int test = 0; test < testCase; test++) {
		cin >> N >> M;
		U = (M * 2) - N;
		T = M - U;
		cout << U << " " << T << '\n';
	}

	return 0;
}
