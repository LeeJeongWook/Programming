#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>


using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int cnt = 0;
	string str;

	while (getline(cin, str)) {
		cnt++;
	}

	cout << cnt;

	return 0;
}
