#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);
	
	int a, b;
	
	while (1) {
		cin >> a >> b;
		if ((a == 0) && (b == 0))
			break;
		cout << a / b << " " << a % b << " / " << b << '\n';
	}
	
    return 0;
}
