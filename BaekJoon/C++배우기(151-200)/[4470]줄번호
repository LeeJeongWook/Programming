#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	int N;
	string str;

	cin >> N;
	cin.ignore();

	for (int test = 1; test <= N; test++) {
		getline(cin, str);
		cout << test << ". " << str << endl;
	}

	return 0;
}
