#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	freopen("input.txt", "r", stdin);

	int T, idx;
	string str;

	cin >> T;
	
	for (int test = 0; test < T; test++) {
		cin >> idx >> str;
		str[idx - 1];
		str.erase(str.begin() + (idx-1));
		cout << str << '\n';
	}

	return 0;
}
