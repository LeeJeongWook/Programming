#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	freopen("input.txt", "r", stdin);

	string s;
	int N, tmp;

	cin >> N;
	cin.ignore();
	for (int test = 0; test < N; test++) {
		getline(cin, s);
		s[0] = toupper(s[0]);
		cout << s << '\n';
	}

	return 0;
}
