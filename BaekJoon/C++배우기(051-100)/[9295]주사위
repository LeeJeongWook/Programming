#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	freopen("input.txt", "r", stdin);

	int testCase, a, b;
	
	cin >> testCase;

	for (int test = 1; test <= testCase; test++) {
		cin >> a >> b;
		cout << "Case " << test << ": " << a + b << endl;
	}
	
	return 0;
}
