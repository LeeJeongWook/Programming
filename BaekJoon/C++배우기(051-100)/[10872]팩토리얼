#define _CRT_SECURE_NO_DEPRECATE
#include <iostream>
#include <vector>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	freopen("input.txt", "r", stdin);

	int N, ans = 1;
	
	cin >> N;

	for (int i = 1; i <= N; i++) {
		ans *= i;
	}

	cout << ans;

	return 0;
}
